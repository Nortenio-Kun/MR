            android:layout_width="wrap_content"
            android:layout_height="110dp"
            android:gravity="center_vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageButton
                android:id="@+id/fab4_2"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_margin="4dp"
                android:background="@drawable/pintura"
                android:tint="@android:color/white" />

            <ImageButton
                android:id="@+id/fab4_4"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_margin="4dp"
                android:background="@drawable/albanil"
                android:tint="@android:color/white" />


            <ImageButton
                android:id="@+id/fab4_main"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:background="@drawable/servicios"
                android:elevation="10dp" />

            <ImageButton
                android:id="@+id/fab4_3"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_margin="4dp"
                android:background="@drawable/foco"
                android:tint="@android:color/white" />

            <ImageButton
                android:id="@+id/fab4_5"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_margin="4dp"
                android:background="@drawable/otros"
                android:tint="@android:color/white" />

        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>